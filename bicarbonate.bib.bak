  issue           = {6 Pt 1},
  keywords        = {Adult; Carbon Dioxide, analysis, metabolism; Carbon Isotopes; Carbon Radioisotopes; Female; Humans; Infusions, Intravenous; Isotope Labeling; Kinetics; Leucine, metabolism; Male; Radioisotope Dilution Technique; Reference Values; Respiration; Sodium Bicarbonate, administration & dosage, metabolism},
  nlm-id          = {0370511},
