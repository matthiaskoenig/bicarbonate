  issue           = {3},
  keywords        = {Adult; Analysis of Variance; Bicarbonates, pharmacokinetics; Caproates, blood, metabolism; Carbon Isotopes; Exercise, physiology; Food; Humans; Keto Acids, blood, metabolism; Leucine, metabolism; Male; Oxidation-Reduction; Protein Biosynthesis; Pulmonary Gas Exchange},
  nlm-id          = {8005433},
