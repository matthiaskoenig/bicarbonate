  issue           = {4 Pt 1},
  keywords        = {Adult; Bicarbonates, metabolism; Carbon Dioxide, blood, metabolism, urine; Carbon Radioisotopes; Energy Metabolism; Forecasting; Humans; Male; Osmolar Concentration; Respiration; Saliva, metabolism},
  nlm-id          = {0370511},
